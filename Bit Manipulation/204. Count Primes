//BRUTE FORCE
class Solution {
public:
    bool isPrime(int n){
        if(n<2) return false; //o and 1 are both not prime
        
        int count=0;
        int i=1;
        while(i<=n){
            if(n%i==0) count++;
            i++;
        }
        if(count>2) return false;
        else return true;
    }

    int countPrimes(int n) {
        int count=0;
        int i=2;

        while(i<n){
            if(isPrime(i)) count++;
            i++;
        }
        return count;
    }
};
